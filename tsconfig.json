{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "jsx": "react-jsx",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "Styles/*": ["src/styles/*"],
      "Components/*": ["src/components/*"],
      "Hooks/*": ["src/hooks/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["./dist"]
}
